{
    "manifest_version": 2,

    "name": "Chrome Tabs Lister. (JN)",
    "description": " ",
    "version": "1.0",
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    
    "permissions": [
        "tabs"
    ],

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}
